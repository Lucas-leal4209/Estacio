// Online C compiler to run C program online
#include <stdio.h>

int main()
{
    int M[4][14];
    
    int i, j;
    
    for (int n=0;n<56;n++){
        i = n / 14;
        j = n % 14;
        
        if (i < j){
            M[i][j] = 2 * i + 7 * j - 2;
        }
        else if (i == j){
            M[i][j] = 3 * i * i - 1;
        }
        else{
            M[i][j] = 4 * i * i * i - 5 * j * j + 1;
        }
    }
    
    for (int n=0;n<56;n++){
        i = n / 14;
        j = n % 14;
        
        printf("%d\t", M[i][j]);
        
        if ((n + 1) % 14 == 0) {
            printf("\n");
        }
    }

    return 0;
}
