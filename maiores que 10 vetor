// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int matriz[4][4];
    int contador;
    for (int i=0;i<4;i++){
        for (int j=0;j<4;j++){
            printf("Insira valores na matrix:");
            scanf("%d",&matriz[i][j]);
        }
    }
    //printf("matriz:\n");
    //for (int i=0;i<4;i++){
    //    for (int j=0;j<4;j++){
    //        printf("%d ",matriz[i][j]);
    //    }
    //printf("\n");
    //}
    for (int i=0;i<4;i++){
        for (int j=0;j<4;j++){
        if (matriz[i][j]>10){
            contador++;
        }    
    }
}
    printf("%d valores maiores que 10.",contador);
    return 0;
}
