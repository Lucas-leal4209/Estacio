// Online C compiler to run C program online
#include <stdio.h>
#include <time.h>

void converterData(struct tm data){
    struct tm *data;
    int dia, mes, ano;
    data.tm_mday=dia;
    data.tm_mon=mes;
    data.tm_year=ano;
    
};

int main() {
    time_t dia;
    time(&dia);
    
    printf("Hello world");

    return 0;
}
