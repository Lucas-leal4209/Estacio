// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int matriz[3][3];
    int x,existe;
    
    printf("Insira os valores da matriz:");
    for (int i=0;i<3;i++){
        for (int j=0;j<3;j++){
            scanf("%d",&matriz[i][j]);
        }
    }
    printf("Informe o valor de x: ");
    scanf("%d",&x);
    for (int i=0;i<3;i++){
        for (int j=0;j<3;j++){
            if (matriz[i][j]==x){
                printf("Valor %d encontrado na linha %d coluna %d.",x,i+1,j+1);
                existe=1;
                break;
            }
        }
    }
    if (existe!=1){
        printf("Valor nÃ£o encontrado na matriz.");
    }
    return 0;
}
