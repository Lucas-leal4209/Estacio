// Online C compiler to run C program online
#include <stdio.h>

int main() {
    double vetor[10];
    int temporario=0;
    for (int i=1;i<=10;i++){
        printf("Informe um valor: ");
        scanf("%lf",&vetor[i]);
    }
    for (int i=1;i<=10;i++){
        if (vetor[temporario] < vetor[i])
        temporario=i;
    }
    
    printf("maior número: %.f",vetor[temporario]);
    printf("\n");
    printf("posição: %d",temporario);
    return 0;
}
