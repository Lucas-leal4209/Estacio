// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int vetor[10];
    int x;
    int multiplos;
    for (int i=1;i<=10;i++){
        printf("Informe um valor: ");
        scanf("%d",&vetor[i]);
    }
    printf("Informe o valor de x: ");
    scanf("%d",&x);
    for (int i=1;i<=10;i++){
        if (vetor[i]%x==0){
            multiplos++;
        }
    }
    
    printf("Existem %d mÃºltiplos de %d no vetor.\n",multiplos,x);

    return 0;
}
