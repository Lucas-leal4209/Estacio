// Online C compiler to run C program online
#include <stdio.h>

int main() {
    double vetor[10];
    int maior=0;
    int menor=0;
    for (int i=0;i<10;i++){
        printf("Informe um valor: ");
        scanf("%lf",&vetor[i]);
    }
    
    maior=vetor[0];
    menor=vetor[0];
    
    for (int i=0;i<10;i++){
        if (vetor[i]>maior) {
            maior=vetor[i];
        }
        if (vetor[i]<menor){
            menor=vetor[i];
        }
    }
    printf("maior número: %d.\n",maior);
    printf("menor número: %d.\n",menor);
    return 0;
}
