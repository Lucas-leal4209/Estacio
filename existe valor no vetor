// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int vetor[]={23, 10, 45, 0, -9, 7, 89, -4, 2, 102, 
14, 0, 36};
    int x;
    int existe;
    printf("Valor:");
    scanf("%d",&x);
    for(int i=0;i<=13;i++){
        if (vetor[i]==x){
        existe=1;
    } 
}
    if (existe>=1){
        printf("Valor está no vetor.");
    } else{
        printf("Valor não está no vetor.");
    }
    return 0;
}
