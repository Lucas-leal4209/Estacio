// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int matriz1[4][4];
    int matriz2[4][4];
    int matriz3[4][4];
    
    
    printf("Insira os valores da primeira matriz: ");
    for (int i=0;i<4;i++){
        for (int j=0;j<4;j++){
            scanf("%d",&matriz1[i][j]);
        }
    }
    printf("Insira os valores da segunda matriz: ");
    for (int i=0;i<4;i++){
        for (int j=0;j<4;j++){
            scanf("%d",&matriz2[i][j]);
        }
    }
    for (int i=0;i<4;i++){
        for (int j=0;j<4;j++){
            if (matriz1[i][j]>matriz2[i][j]){
                matriz3[i][j]=matriz1[i][j];
            }
            else{
                matriz3[i][j]=matriz2[i][j];
            }
        }
    }
    printf("A terceira matriz com os maiores valores Ã©: ");
    for (int i=0;i<4;i++){
        for (int j=0;j<4;j++){
            printf("%d ",matriz3[i][j]);
        } printf("\n");
    }
        
    return 0;
}
