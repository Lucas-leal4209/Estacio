// Online C compiler to run C program online
#include <stdio.h>

int main() {
    float vetor[5];
    int escolha;
    for (int i=0;i<5;i++){
        printf("Insira um valor no vetor: ");
        scanf("%f",&vetor[i]);
    }
    printf("Escolha uma função:\n0)Finalizar o programa\n1)Imprimir o vetor\n2)Imprimir o inverso do vetor\n");
    scanf("%d",&escolha);
    switch (escolha){
        default:
        printf("Fim do programa");
        case 1:
        for (int i=0;i<5;i++){
        printf("%.f ",vetor[i]);
        }
        case 2:
        for (int i=5-1;i>=0;i--){
        printf("%.f ",vetor[i]);
        }
    }
        
    

    return 0;
}
